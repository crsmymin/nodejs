<h2>community</h2>
<a href="/board/write">글쓰기</a>
<table id="listTable">
  <thead>
    <tr>
      <th>글번호</th>
      <th>작성자</th>
      <th>제목</th>
      <th>작성일</th>
    </tr>
  </thead>
  <tbody>
    <% for(i=0; i<list.length; i++) { %> 
    <tr>
      <td><%=i+1 %></td>
      <td><%=list[i].author %></td>
      <td class="tal" id="brdNo<%=list[i].board_no %>">
        <a href="/board/view/<%=list[i].board_no %>">
          <%=list[i].board_title %>
        </a>
      </td>
      <td>
        <%=list[i].created_at.toLocaleDateString() %>
      </td>
    </tr>
    <% } %> 
  </tbody>
</table>